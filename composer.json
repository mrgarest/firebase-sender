{
    "name": "garest/firebase-sender",
    "description": "Laravel library for sending notifications with Firebase Cloud Messaging (FCM)",
    "type": "library",
    "license": "MIT",
    "version": "3.1.3",
    "homepage": "https://github.com/mrgarest/firebase-sender",
    "keywords": [
        "Garest",
        "MrGarest",
        "google",
        "laravel",
        "firebase",
        "fcm",
        "notification",
        "php",
        "android",
        "apns"
    ],
    "authors": [
        {
            "name": "Garest",
            "homepage": "https://github.com/mrgarest"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "google/auth": "^1.49"
    },
    "autoload": {
        "psr-4": {
            "Garest\\FirebaseSender\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Garest\\FirebaseSender\\FirebaseSenderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
